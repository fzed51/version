{
    "name": "fzed51/version",
    "description": "Class permet de manipuler des numeros de version",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Version\\": "src/Version/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Version\\": "test/Version/",
            "Test\\": "test/"
        }
    },
    "authors": [
        {
            "name": "Fabien Sanchez",
            "email": "fzed51+git@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4.0",
      "ext-json": "*",
        "fzed51/console-options": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6",
        "phpstan/phpstan": "^1.0",
        "ocramius/package-versions": "^2.1"
    },
    "scripts": {
        "fix": "phpcbf",
        "check": [
            "phpcs",
            "phpstan analyse"
        ],
        "test": "phpunit"
    }
}
